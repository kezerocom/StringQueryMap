<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <!-- Package Info -->
        <PackageId>StringQueryMap</PackageId>
        <Version>2.0.0</Version>
        <Authors>Alecio Furanze</Authors>
        <Company>KeZero</Company>
        <Product>StringQueryMap</Product>
        <Description>A lightweight library for serializing and deserializing key-value pairs as strings.</Description>
        <PackageTags>key-value;serialization;deserialization;generic;dotnet</PackageTags>
        <RepositoryUrl>https://github.com/kezerocom/StringQueryMap</RepositoryUrl>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageProjectUrl>https://github.com/kezerocom/StringQueryMap</PackageProjectUrl>
        <PackageReadmeFile>readme.md</PackageReadmeFile>
        <!-- <PackageIcon>icon.png</PackageIcon> -->
        <PackageReleaseNotes>
            1.0.0
            - Initial release of StringQueryMap
            2.0.0
            - Renamed project, solution, and namespaces from StringKeyMap to StringQueryMap
            - Updated README and documentation references
        </PackageReleaseNotes>
        <NeutralLanguage>en</NeutralLanguage>
        <!-- Include symbols for debugging -->
        <IncludeSymbols>true</IncludeSymbols>
        <SymbolPackageFormat>snupkg</SymbolPackageFormat>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    </PropertyGroup>

    <ItemGroup>
        <None Include="../readme.md" Pack="true" PackagePath="/"/>
        <None Include="../license.md" Pack="true" PackagePath="/"/>
    </ItemGroup>

</Project>
